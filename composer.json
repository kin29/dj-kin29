{
    "name": "kin29/dj-kin29",
    "description": "Web Application for creating automatically Spotify playlist",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "composer-plugin-api": "^1.0 || ^2.0",
        "jwilsson/spotify-web-api-php": "^5.0",
        "phpunit/phpunit": "^9.5",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/debug-pack": "^1.0",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/intl": "5.3.*",
        "symfony/mailer": "5.3.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "^2.0",
        "symfony/process": "5.3.*",
        "symfony/profiler-pack": "^1.0",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/translation": "5.3.*",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "5.3.*",
        "symfony/web-link": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpmetrics/phpmetrics": "^2.6",
        "phpspec/prophecy-phpunit": "^2.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform-check": false
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "test": [
            "./vendor/bin/phpunit tests/"
        ],
        "cs": "vendor/bin/php-cs-fixer fix -v --dry-run src/",
        "cs-fix": "vendor/bin/php-cs-fixer fix -v src/"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
